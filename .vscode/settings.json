{
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "python.formatting.provider": "none",
    "rust-analyzer.linkedProjects": [
        "candle-flash-attn/Cargo.toml",
        "infer/Cargo.toml",
        "aicirt/Cargo.toml",
        "aici_abi/Cargo.toml",
        "aici_tokenizers/Cargo.toml",
        "aici_ast_runner/Cargo.toml",
        "pyvm/Cargo.toml"
    ],
    "rust-analyzer.diagnostics.disabled": [],
    "files.associations": {
        "*.rasi": "css",
        "*.bbmodel": "json",
        "*.sublime-snippet": "xml",
        "*.hbs": "html",
        "*.ejs": "html",
        "lesskey": "lesskey",
        "*.Xresources": "xdefaults",
        "i3/config": "i3",
        "i3/*.conf": "i3",
        "polybar/config": "ini",
        "polybar/*.conf": "ini",
        "*.inc": "arm",
        "Kconfig": "kconfig",
        "__hash_table": "cpp",
        "__split_buffer": "cpp",
        "__tree": "cpp",
        "array": "cpp",
        "bitset": "cpp",
        "deque": "cpp",
        "initializer_list": "cpp",
        "map": "cpp",
        "queue": "cpp",
        "span": "cpp",
        "stack": "cpp",
        "string": "cpp",
        "string_view": "cpp",
        "unordered_map": "cpp",
        "vector": "cpp",
        "format": "cpp"
    },
    "python.analysis.extraPaths": [
        "./vllm",
        "./pyvm/aici-pylib"
    ],
    "spellright.language": [
        "en"
    ],
    "spellright.documentTypes": [
        "markdown",
        "latex",
        "plaintext",
        "rust"
    ],
    "cSpell.words": [
        "aici",
        "aicirt",
        "logit",
        "logits",
        "POSIX",
        "pyaici",
        "struct",
        "subword",
        "usize",
        "vllm",
        "waitpid",
        "wprintln"
    ],
    "mypy.configFile": "vllm/mypy.ini"
}